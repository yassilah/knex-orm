{
   "name": "@yassi/knex-orm",
   "type": "module",
   "version": "0.1.0",
   "description": "TypeScript ORM on top of Knex with schema definition and migrations",
   "author": "Yassi Lah",
   "license": "MIT",
   "keywords": [
      "orm",
      "knex",
      "typescript",
      "postgres",
      "mysql",
      "sqlite"
   ],
   "sideEffects": false,
   "exports": {
      ".": {
         "types": "./dist/index.d.ts",
         "import": "./dist/index.js",
         "require": "./dist/index.js"
      }
   },
   "main": "dist/index.js",
   "types": "dist/index.d.ts",
   "bin": {
      "knex-orm-migrate": "dist/bin/orm-migrate.js"
   },
   "files": [
      "dist"
   ],
   "engines": {
      "node": ">=18"
   },
   "scripts": {
      "build": "tsc",
      "dev": "tsx watch src/index.ts",
      "lint": "eslint . --max-warnings=0",
      "typecheck": "tsc --noEmit",
      "test": "vitest run"
   },
   "dependencies": {
      "knex": "^3.1.0",
      "mysql2": "^3.15.3",
      "sqlite3": "^5.1.7"
   },
   "devDependencies": {
      "better-sqlite3": "^12.4.1",
      "mysql": "^2.18.1",
      "pg": "^8.16.3",
      "pg-mem": "^3.0.5"
   }
}
